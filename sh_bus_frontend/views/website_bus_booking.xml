                                            </select>
                                            <span class="fa fa-caret-down"></span>
                                        </div>
                                        <div class="col-lg-3 col-md-6 col-sm-12 col-12 text-center">
                                            <select name="end_point" id="end_point" class="selectpicker" required="required">
                                                <option value="" disabled="disabled">Choose End Point</option>
                                                <t t-foreach="to_points" t-as="to_point">
                                                    <option t-att-selected="'selected' if int(to_loc) == int(to_point.id) else None" t-att-value="to_point.id">
                                                        <t t-esc="to_point.name"/>
                                                    </option>
                                                </t>
                                            </select>
                                            <span class="fa fa-caret-down"></span>
                                        </div>
                                        <div class="col-lg-2 col-md-6 col-sm-12 col-12 text-center">
                                            <input type="date" t-att-value="search_date" name="date" id="datepicker" placeholder="Date" class="form-control" required="required"></input>
                                        </div>
                                        <div class="col-lg-2 col-md-6 col-sm-12 col-12 text-center pl10 pr10">
                                            <select name="fleet_type" id="fleet_type" class="selectpicker">
                                                <t t-if="fleet_type">
                                                    <option value="">Choose Fleet Type</option>
                                                    <t t-foreach="bus_types" t-as="bus_type">
                                                        <option t-att-selected="'selected' if fleet_type and int(fleet_type) == int(bus_type.id) else None" t-att-value="bus_type.id">
                                                            <t t-esc="bus_type.name"/>
                                                        </option>
                                                    </t>
                                                </t>
                                                <t t-else="">
                                                    <option value="" disabled="disabled" selected="selected">Choose Fleet Type</option>
                                                    <t t-foreach="bus_types" t-as="bus_type">
                                                        <option t-att-value="bus_type.id">
                                                            <t t-esc="bus_type.name"/>
                                                        </option>
                                                    </t>
                                                </t>
                                            </select>
                                            <span class="fa fa-caret-down"></span>
                                        </div>
                                        <div class="col-lg-2 col-md-12 col-sm-12 col-12 text-center">
                                            <button type="submit" class="btn btn_search">Search</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </section>
                    <section id="sh_bus_booking_detail_table">
                        <div class="container">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Route</th>
                                        <th>From</th>
                                        <th>Departure</th>
                                        <th>To</th>
                                        <th>Arrival</th>
                                        <th>Price</th>
                                        <th>Total Seats</th>
                                        <th>Seat Booked</th>
                                        <th>Remaining Seats</th>
                                        <th>Operator</th>
                                    </tr>
                                </thead>
                                <t t-foreach="search_ids" t-as="search_id">
                                    <tr>
                                        <td>
                                            <t t-esc="search_id.route.baording_id.name"/>-
                                            <t t-esc="search_id.route.dropping_id.name"/>
                                        </td>
                                        <td>
                                            <t t-esc="search_id.bording_from.name" />
                                        </td>
                                        <td>
                                            <t t-esc="search_id.trip_start_date" t-options="{'widget': 'float_time'}"/>
                                        </td>
                                        <td>
                                            <t t-esc="search_id.to.name" />
                                        </td>
                                        <td>
                                            <t t-esc="search_id.trip_end_date" t-options="{'widget': 'float_time'}"/>
                                        </td>
                                        <td>
                                            <t t-esc="search_id.price"/>
                                        </td>
                                        <td>
                                            <t t-esc="search_id.total_seat"/>
                                        </td>
                                        <td>
                                            <t t-esc="search_id.seat_booked"/>
                                        </td>
                                        <td>
                                            <t t-esc="search_id.remaining_seats"/>
                                        </td>
                                        <td class="text-center">
                                            <a href="#" class="btn btn-primary book_ticket" style="font-size:14px;"  data-toggle="modal" 
					                   			  t-att-data-target="'#myModal%s00' %(search_id.id)" t-att-data-id="'%s' %(search_id.id)">Book</a>
                                            <t t-call="sh_bus_frontend.seat_booking_template"></t>
                                        </td>
                                    </tr>
                                </t>
                            </table>
                        </div>
                    </section>
                </t>
                <t t-else="">
                    <section id="sh_result_notfound_banner">
                        <div class="container">
                            <div class="box">
                                <h3>No Result Found !</h3>
                            </div>
                        </div>
                    </section>
                </t>
    </t>
    <div class="oe_structure oe_empty oe_structure_not_nearest" id="oe_structure_bus_booking_2"/>
        </div>
    </t>
    
    </template>
</odoo>
