<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_Exp_health_certificate_session" model="ir.cron">
            <field name="name">Exp health certificate</field>
            <field name="model_id" ref="model_hr_employee" />
            <field name="state">code</field>
            <field name="code">model.mail_reminder_for_kb_Exp_health_certificate()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="active" eval="False" />
        </record>

        <record id="ir_kb_Exp_programme_session" model="ir.cron">
            <field name="name">Exp programme</field>
            <field name="model_id" ref="model_hr_employee"/>
            <field name="state">code</field>
            <field name="code">model.mail_reminder_for_kb_Exp_programme()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="active" eval="False" />
        </record>
    </data>

</odoo>