msgstr ""

#. module: kb_hr_forms
#: model_terms:ir.ui.view,arch_db:kb_hr_forms.print_effective_date_notice
msgid ""
"<b>إشعار مباشرة العمل\n"
"                            <br/>\n"
"                            Effective Date Notice </b>"
msgstr ""

#. module: kb_hr_forms
