<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="users_form_whatsapp_form_view" model="ir.ui.view">
        <field name="name">res.users.channel.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="priority">22</field>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_show_rules']" position="after">
                <button name="action_open_channels" string="Whatsapp Channels" type="object" groups="base.group_no_one" class="oe_stat_button" icon="fa-list-ul"/>
            </xpath>
        </field>
    </record>
</odoo>