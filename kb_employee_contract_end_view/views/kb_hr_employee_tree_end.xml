<odoo >
    <data >
        <record model = "ir.ui.view" id = "employee_end_date_view" >
            <field name = "name" >Contract End Date View tree</field >
            <field name = "model" >hr.employee</field >
            <field name = "inherit_id" ref = "hr.view_employee_tree" />
            <field name = "arch" type = "xml" >


                <xpath expr = "//tree" position = "inside" >
                   <field name="end_date"/>
                </xpath >
            </field >
        </record >
    </data >
</odoo >
